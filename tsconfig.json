{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "strictNullChecks": false,
    "baseUrl": "./",
    "types": ["unplugin-vue-macros/macros-global"],
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@ant-design-x-vue": ["src/index.ts"],
    }
  },
  "vueCompilerOptions": {
    "plugins": ["unplugin-vue-macros/volar"]
  },
  "include": [".dumirc.ts", "src/**/*"]
}
